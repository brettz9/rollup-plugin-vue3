{
  "name": "@pathscale/rollup-plugin-vue3",
  "version": "0.0.3",
  "description": "Rollup plugin for .vue files",
  "keywords": [
    "rollup",
    "rollup-plugin",
    "vue"
  ],
  "homepage": "https://github.com/pathscale/rollup-plugin-vue3#readme",
  "bugs": {
    "url": "https://github.com/pathscale/rollup-plugin-vue3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pathscale/rollup-plugin-vue3.git"
  },
  "license": "MIT",
  "author": "Anton Kudryavtsev <boblobl4@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc",
    "prebuild": "shx rm -rf dist",
    "build": "rollup -c --environment NODE_ENV:production",
    "commit": "commit",
    "predocs": "npm run build && shx rm -rf docs",
    "docs": "typedoc dist",
    "postdocs": "shx cp .ghpages.yml docs/_config.yml",
    "docs:publish": "npm run docs && gh-pages -d docs",
    "prelint": "prettier -l --write .",
    "lint": "eslint --ext .ts,.js,.mjs --fix .",
    "prepublishOnly": "npm run build",
    "release": "semantic-release",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@rollup/pluginutils": "^3.1.0",
    "debug": "^4.1.1",
    "fs-extra": "^9.0.1",
    "query-string": "6.13.1",
    "resolve": "^1.17.0",
    "source-map": "^0.7.3",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-angular": "^9.1.1",
    "@commitlint/prompt-cli": "^9.1.1",
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "*",
    "@types/resolve": "^1.20.6",
    "builtin-modules": "^3.1.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "husky": "^4.2.5",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "less": "^3.12.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rollup": "^2.23.1",
    "rollup-plugin-dts": "^1.4.10",
    "rollup-plugin-node-externals": "^2.2.0",
    "sass": "^1.26.10",
    "scss": "^0.2.4",
    "semantic-release": "^17.1.1",
    "shx": "^0.3.2",
    "stylus": "^0.54.8",
    "ts-jest": "^29.3.0",
    "typedoc": "^0.18.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "rollup": "^2.2.0"
  },
  "engines": {
    "node": "^20.11.0 || >=22.0.0"
  }
}
